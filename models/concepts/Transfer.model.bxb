structure (Transfer) {
  description(A transfer.)
  
  property (toAccount) {
    type (ToAccount)
    min(Required)
  }
  
  property (fromAccount) {
    type (FromAccount)
    min(Required)
  }
  
  property (transferAmount) {
    min(Required)
    type(TransferAmount)
  }
  
  property (note) {
    type(Note)
  }
  features { transaction }
}