dialog (Value) {
  match: AccountType (this)
  switch (this) {
    case (Checking) {
      template-macro (Checking)
    }
    case (Savings) {
      template-macro (Savings)
    }
    case (MoneyMarket) {
      template-macro (MoneyMarket)
    }
    default {
      template-macro (UnknownAccountType)
    }
  }
}