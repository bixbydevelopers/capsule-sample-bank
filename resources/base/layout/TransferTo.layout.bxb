layout-macro-def (transfer-to) {
  params {
    param (transfer) {
      type (Transfer)
      min (Required) max (One)
    }
  }
  content {
    section {
      title {
        template-macro (SendToLabel)
      }
      content {
        single-line {
          text {
            value ("#{value(transfer.toAccount.accountType)}")
          }
        }
      }
    }
  }
}